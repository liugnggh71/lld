<?xml version="1.0" encoding="UTF-8"?>

<binary name="saxon_he_stable.jar" subdir="dba_code/bin">
    <version>11.4</version>
    <stage_dir>/tmp/saxon_he</stage_dir>
    <wget_zip>https://github.com/Saxonica/Saxon-HE/raw/main/11/Java/SaxonHE11-4J.zip</wget_zip>
    <commands>
        <command>
            <mkdir_stage/>
        </command>
        <command>
            <cd_stage/>
        </command>
        <command>
            <rm_wget_zip/>
        </command>
        <command>
            <wget_zip/>
        </command>
        <command>
            <unzip_wget_zip/>
        </command>
        <command>
            <cp_single_pick copy_from="saxon-he-11.4.jar"/>
        </command>
    </commands>
</binary>
