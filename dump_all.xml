<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="binary.xsl"?>
<!-- Driver XML -->
<binarys xmlns:xi="http://www.w3.org/2001/XInclude"
         git="https://github.com/liugnggh71/lld/raw/main">
   <stage>
      <wget_stage_code>install_binary_lld.sh</wget_stage_code>
   </stage>
   <binary name="saxon_he_stable.jar"
           subdir="dba_code/bin"
           xml:base="code/binary/Saxon-HE_Java.xml">
      <version>11.4</version>
      <stage_dir>/tmp/saxon_he</stage_dir>
      <wget_zip>https://github.com/Saxonica/Saxon-HE/raw/main/11/Java/SaxonHE11-4J.zip</wget_zip>
      <commands>
         <command>
            <set_pwd/>
         </command>
         <command>
            <mkdir_stage/>
         </command>
         <command>
            <cd_stage/>
         </command>
         <command>
            <rm_wget_zip/>
         </command>
         <command>
            <wget_zip/>
         </command>
         <command>
            <unzip_wget_zip/>
         </command>
         <command>
            <cp_single_pick copy_from="saxon-he-11.4.jar"/>
         </command>
         <command>
            <cd_pwd/>
         </command>
      </commands>
   </binary>
</binarys>
